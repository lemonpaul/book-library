{% extends 'base.html.twig' %}

{% block main %}
<div>
    <h1>Add Book</h1>

    {{ form_start(form) }}
        {{ form_row(form.title, { class: 'form-group' }) }}
        {{ form_row(form.author) }}
        {{ form_row(form.cover) }}
        {{ form_row(form.file) }}
        {{ form_row(form.date) }}
        {{ form_row(form.download) }}
    {{ form_end(form) }}
</div>
{% endblock %}
